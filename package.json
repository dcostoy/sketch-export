{
  "name": "sketch-export-to",
  "description": "Export an artboard layers to json or markdown",
  "version": "2.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-export-to",
    "manifest": "src/manifest.json",
    "main": "sketch-export-to.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "@skpm/builder": "^0.8.0",
    "eslint": "^8.8.0"
  },
  "author": "Demetrios Costoyiannis <demetrios@gmail.com>",
  "dependencies": {
    "@skpm/dialog": "^0.2.4",
    "@skpm/fs": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dcostoy/sketch-export.git"
  }
}
